{
  "comment": "Staging environment configuration - production-like setup for testing",
  "environment": "staging",
  "aws": {
    "region": "us-east-1",
    "account_id": "YOUR_STAGING_ACCOUNT_ID"
  },
  "bastion_host": {
    "instance_type": "t3.small",
    "enabled": false
  },
  "stream_processor": {
    "parallelism": 6,
    "memory_per_taskmanager_mb": 2048
  },
  "data_lake": {
    "enable_intelligent_tiering": true
  },
  "observability": {
    "log_level": "INFO",
    "cloudwatch_log_retention_days": 14
  },
  "tags": {
    "Environment": "staging"
  }
}
